<?xml version="1.0" encoding="utf-8"?>
<DHTest>
	<!--工程管理-->
	<Module type ="ProjectModule" id ="ProjectModuleAnalysis" Default="1">
		<menu id ="ProjectModuleAnalysis" analysis="1">
			<toolbar>
				<Control id="NewPrjFunc" switch="MeasureModule|MeasureModuleAnalysis"/>
			</toolbar>
			<toolbar>
				<Control id="OpenPrjFunc"  switch="ModalDataModule|ModalDataModuleAnalysis"/>
