        <Control id="ChatVertFunc"/>
      </toolbar>
      <toolbar>
        <Control id="LayoutGroupbuttonAnalysis" chartMenu="Modaltestmenu_chart.xml" type="Layout"/>
      </toolbar>
      <toolbar>
        <Control id="ChartGroupbuttonAnalysis" chartMenu="Modaltestmenu_chart.xml" type="Chart"/>
      </toolbar>
			<toolbar>
				<Control id="RunInfoFunc"/>
			</toolbar>
		</menu>
	</Module>
	<!--截取-->
	<Module type ="DataDelModule" id ="DataDelModuleAnalysis">
		<menu id ="DataDelModuleAnalysis" analysis ="1">
			<toolbar>
				<Control id="ModalDataDelFunc"/>
			</toolbar>
		</menu>
	</Module>
	<!--模态数据-->
	<Module type ="ModalDataModule" id ="ModalDataModuleAnalysis">
		<menu id ="ModalDataModuleAnalysis" analysis ="1">
			<toolbar>
				<Control id="ModalFileManageFunc"/>
