      </toolbar>
      <toolbar>
        <Control id="LayoutGroupbuttonAnalysis" chartMenu="StudentMenu_chart.xml" type="Layout"/>
      </toolbar>
      <toolbar>
        <Control id="ChartGroupbuttonAnalysis" chartMenu="StudentMenu_chart.xml" type="Chart"/>
      </toolbar>
			<toolbar>
				<Control id="RunInfoFunc"/>
			</toolbar>
		</menu>
