<?xml version="1.0" encoding="utf-8"?>
<DHTest>
	<!--工程管理-->
	<Module type ="ProjectModule" id ="ProjectModuleAnalysis" Default="1">
		<menu id ="ProjectModuleAnalysis" analysis="1">
			<toolbar>
				<Control id="NewPrjFunc" switch="MeasureModule|ChnSetupModuleAnalysis"/>
			</toolbar>
			<toolbar>
				<Control id="OpenPrjFunc" switch="AnalysisModule|AnalysisModuleAnalysis"/>
				<Control id="RenamePrjFunc"/>
				<Control id="DelPrjFunc"/>
			</toolbar>
		</menu>
	</Module>
	<!--测量-->
	<Module type ="MeasureModule" id ="MeasureModuleAnalysis">
		<!--通道设置-->
		<menu id ="ChnSetupModuleAnalysis" analysis ="0">
			<toolbar>
				<Control id="SaveParamFunc"/>
				<Control id="SaveToParamFunc"/>
			</toolbar>
			<toolbar>
				<Control id="AnalogParamModuleAnalysis" UserControl ="1"/>
				<Control id="TachoParamModuleAnalysis" UserControl ="1"/>
				<Control id="SignalParamModuleAnalysis" UserControl ="1"/>
				<Control id="GPSParamModuleAnalysis" UserControl ="1"/>
				<Control id="CANParamModuleAnalysis" UserControl ="1"/>
				<Control id="OutDataParamModuleAnalysis" UserControl ="1"/>
				<Control id="VideoParamModuleAnalysis" UserControl ="1"/>
				<Control id="AudioParamModuleAnalysis" UserControl ="1"/>
				<Control id="CalibrateParamModuleAnalysis" UserControl ="1"/>
				<Control id="DigitalParamModuleAnalysis" UserControl ="1"/>
				<Control id="DIOParamModuleAnalysis" UserControl ="1"/>
			</toolbar>
		</menu>
		<!--算法设置-->
		<menu id ="MathSetupModuleAnalysis" analysis ="0">
			<toolbar>
				<Control id="SaveParamFunc"/>
				<Control id="SaveToParamFunc"/>
			</toolbar>
			<toolbar>
				<!--<Control id="GeneralMathModuleAnalysis"/>-->
				<Control id="MathSelfDefineModuleAnalysis"/>
				<Control id="SpectrumMathModuleAnalysis"/>
				<Control id="CableMathModuleAnalysis"/>
				<Control id="OrderMathModuleAnalysis"/>
				<Control id="StableMathModuleAnalysis"/>
				<Control id="AcousticsMathModuleAnalysis"/>
			</toolbar>
		</menu>
		<menu id ="MeasureModuleAnalysis" analysis ="0">
			<toolbar>
				<Control id="MeasureStoreFunc"/>
				<Control id="MeasurePauseFunc"/>
				<Control id="MeasureContinueFunc"/>
				<Control id="MeasureStopFunc"/>
				<Control id="MeasureShiBoFunc"/>
				<Control id="MeasureTrigFunc"/>
				<Control id="MeasureFreezeFunc"/>
				<Control id="MeasureBalanceZero"/>
				<Control id="MeasureAutoRand"/>
				<Control id="MeasureTagFunc"/>
			</toolbar>
      <toolbar>
        <Control id="DelAllChatFunc"/>
        <Control id="DelChatFunc"/>
        <Control id="ChatHorizFunc"/>
        <Control id="ChatVertFunc"/>
      </toolbar>
      <toolbar>
        <Control id="MultiScreenFunc"/>
      </toolbar>
      <toolbar>
        <Control id="LayoutGroupbuttonAnalysis" chartMenu="FremdMenu_chart.xml" type="Layout"/>
      </toolbar>
      <toolbar>
        <Control id="ChartGroupbuttonAnalysis" chartMenu="FremdMenu_chart.xml" type="Chart"/>
      </toolbar>
			<toolbar>
				<Control id="RunInfoFunc"/>
			</toolbar>
		</menu>
	</Module>
	<!--分析-->
	<Module type ="AnalysisModule" id ="AnalysisModuleAnalysis">
		<!--通道-->
		<menu id ="ChannelModuleAnalysis" analysis ="1">
			<toolbar>
				<Control id="EditChannelFunc"/>
			</toolbar>
		</menu>
		<!--事件-->
		<menu id ="EventModuleAnalysis" analysis ="1">
			<toolbar>
				<Control id="AddEventFunc"/>
				<Control id="EditEventFunc"/>
				<Control id="DelEventFunc"/>
			</toolbar>
		</menu>
		<menu id ="MathSetupModuleAnalysis" analysis ="1">
			<toolbar>
				<!--<Control id="GeneralMathModuleAnalysis"/>-->
				<Control id="MathSelfDefineModuleAnalysis"/>
				<Control id="SpectrumMathModuleAnalysis"/>
				<Control id="CableMathModuleAnalysis"/>
				<Control id="OrderMathModuleAnalysis"/>
				<Control id="StableMathModuleAnalysis"/>
				<Control id="AcousticsMathModuleAnalysis"/>
				<Control id="FatigueMathModuleAnalysis"/>
			</toolbar>
		</menu>
		<menu id ="AnalysisModuleAnalysis" analysis ="1" Default="1">
			<toolbar>
				<Control id="AnalysisStartPlaybackFunc"/>
				<Control id="AnalysisSlowPlaybackFunc"/>
				<Control id="AnalysisRatePlaybackFunc"/>
				<Control id="AnalysisStepPlaybackFunc"/>
				<Control id="AnalysisStopPlaybackFunc"/>
				<Control id="MeasureTagFunc"/>
			</toolbar>
			<toolbar>
				<Control id="AnalysisHandCalFunc"/>
				<Control id="AnalysisAutoCalFunc"/>
			</toolbar>
      <toolbar>
        <Control id="DelAllChatFunc"/>
        <Control id="DelChatFunc"/>
        <Control id="ChatHorizFunc"/>
        <Control id="ChatVertFunc"/>
      </toolbar>
      <toolbar>
        <Control id="MultiScreenFunc"/>
      </toolbar>
      <toolbar>
        <Control id="LayoutGroupbuttonAnalysis" chartMenu="FremdMenu_chart.xml" type="Layout"/>
      </toolbar>
      <toolbar>
        <Control id="ChartGroupbuttonAnalysis" chartMenu="FremdMenu_chart.xml" type="Chart"/>
      </toolbar>
			<toolbar>
				<Control id="RunInfoFunc"/>
			</toolbar>
		</menu>
		<!--输出-->
		<menu id ="OutputModuleAnalysis" analysis="1">
			<toolbar>
				<Control id="ExcelOutputModuleAnalysis" UserControl ="1"/>
				<Control id="MatLabOutputModuleAnalysis" UserControl ="1"/>
				<Control id="TxtOutputModuleAnalysis" UserControl ="1"/>
				<Control id="UFFOutputModuleAnalysis" UserControl ="1"/>
				<Control id="AcessOutputModuleAnalysis" UserControl ="1"/>
			</toolbar>
			<toolbar>
				<Control id="WordOutputModuleAnalysis" UserControl ="1"/>
				<Control id="PicOutputModuleAnalysis" UserControl ="1"/>
			</toolbar>
			<toolbar>
				<Control id="DHSoftOutputModuleAnalysis" UserControl ="1"/>
				<Control id="ReportOutputModuleAnalysis" UserControl ="1"/>
			</toolbar>
			<toolbar>
				<Control id="OutOutputFunc"/>
			</toolbar>
		</menu>
	</Module>
	<!--模态-->
	<Module type ="DataDelModule" id ="ModalAnalysis">
		<!--建模-->
		<menu id ="ModelModuleAnalysis" analysis="1">
			<toolbar>
				<Control id="ImportModelFunc"/>
				<Control id="SaveModelFunc"/>
				<Control id="SaveToModelFunc"/>
			</toolbar>
			<toolbar>
				<Control id="EditModelFunc"/>
			</toolbar>
			<toolbar>
				<Control id="ShowModelFunc"/>
			</toolbar>
			<toolbar>
				<Control id="ProcessionalModelFunc"/>
			</toolbar>
		</menu>
		<!--截取-->
		<Module type ="DataDelModule" id ="DataDelModuleAnalysis">
			<menu id ="DataDelModuleAnalysis" analysis ="1">
				<toolbar>
					<Control id="ModalDataDelFunc"/>
				</toolbar>
			</menu>
		</Module>
		<!--模态数据-->
		<menu id ="ModalDataModuleAnalysis" analysis ="1">
			<toolbar>
				<Control id="ModalFileManageFunc"/>
			</toolbar>
			<toolbar>
				<Control id="ModalMeasureNoForceModuleAnalysis"/>
				<Control id="ModalMeasureForceModuleAnalysis"/>
			</toolbar>
			<toolbar>
				<Control id="ModalSingleInMultiOutFunc"/>
				<Control id="ModalMultiInSingleOutFunc"/>
				<Control id="ModalMultiInMultiOutFunc"/>
			</toolbar>
		</menu>
		<!--模态参数识别New-->
		<menu id ="ModalIndentifyXModuleAnalysis" analysis ="1">
			<toolbar>
				<Control id="ModalIndentifyDataFunc"/>
			</toolbar>
			<toolbar>
				<Control id="ModalIndentifyIDSelFunc"/>
			</toolbar>
			<toolbar>
				<Control id="ModalIndentifyFFTAnalysis" UserControl="1"/>
				<Control id="ModalIndentifySelFreqAnalysis" UserControl="1"/>
				<Control id="ModalIndentifyModalIdenAnalysis" UserControl="1"/>
				<Control id="ModalIndentifyCorrelationAnalysis" UserControl="1"/>
				<Control id="ModalIndentifyDumpAnalysis" UserControl="1"/>
				<Control id="ModalIndentifySelFreqWidthAnalysis" UserControl="1"/>
				<Control id="ModalIndentifyStablyPicAnalysis" UserControl="1"/>
				<Control id="ModalIndentifyPloyAnalysis" UserControl="1"/>
			</toolbar>
		</menu>
		<!--振型-->
		<menu id ="ModalVibModelModuleAnalysis" analysis ="1">
			<toolbar>
				<Control id="ShowModelFunc"/>
			</toolbar>
			<toolbar>
				<Control id="ModalVibShapeCollateFunc"/>
			</toolbar>
			<toolbar>
				<Control id="ModalVibFunc"/>
			</toolbar>
			<toolbar>
				<Control id="ModalVibControlFunc"/>
			</toolbar>
		</menu>
		<!--模态验证-->
		<menu id ="ModalModelValidateModuleAnalysis" analysis ="1">
			<toolbar>
				<Control id="ModalIndentifyDataFunc"/>
			</toolbar>
			<toolbar>
				<Control id="ModalValidateMACModuleAnalysis" UserControl="1"/>
			</toolbar>
			<toolbar>
				<Control id="ModalValidateMSFModuleAnalysis" UserControl="1"/>
			</toolbar>
			<toolbar>
				<Control id="ModalValidateMOVModuleAnalysis" UserControl="1"/>
			</toolbar>
		</menu>
	</Module>
	<!--切换定义-->
	<Switch>
		<SwitchItem id="SwitchMeasurePath" path="MeasureModule|MeasureModuleAnalysis"></SwitchItem>
		<SwitchItem id="SwitchAnalysisPath" path="AnalysisModule|AnalysisModuleAnalysis"></SwitchItem>
    <SwitchItem id="SwitchChnAnalogPath" path="MeasureModule|ChnSetupModuleAnalysis|AnalogParamModuleAnalysis"></SwitchItem>
	</Switch>
</DHTest>